{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.log": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.map": {"when": "$(basename).map"}
    }
}